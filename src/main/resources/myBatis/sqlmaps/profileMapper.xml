<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Profile">

    <insert id="saveProfileImg"
            parameterType="com.green.vo.ProfileVo">
        INSERT INTO profile_tb (_id, profiledata)
        VALUES                 (#{_id}, #{profiledata})

    </insert>

    <select id="selectImg"
            resultType="String">
        SELECT profiledata
        FROM   profile_tb
        WHERE  profiledata = #{userFilename}

    </select>


</mapper>