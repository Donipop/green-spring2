<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//Dli Mapper 3.0//EN"
        "http://mybatis.org/dli/mybatis-3-mapper.dtd">

<mapper namespace="Msg">
    <select id="selectRecept"
            resultType="com.green.vo.NoteVo">
        SELECT *
        FROM MSG
        WHERE RECEPT = #{recept}
    </select>

    <insert id="insertUser"
            parameterType="com.green.vo.NoteVo">
        INSERT INTO MSG (_ID, CONTENT, RECEPT, SEND, TIME ) VALUES (#{_id},#{content}, #{recept}, #{send}, #{time})
    </insert>

</mapper>
